# Eufy credentials
EUFY_USERNAME=your_email@example.com
EUFY_PASSWORD=your_password

# Model selection: "claude" or "gemini" (default: claude)
MODEL=claude

# Anthropic API key (required if MODEL=claude)
ANTHROPIC_API_KEY=sk-ant-...

# Google AI API key (required if MODEL=gemini)
GOOGLE_AI_API_KEY=your_google_ai_api_key

# MQTT configuration (for local broker)
MQTT_HOST=localhost
MQTT_PORT=2000
MQTT_USER=user
MQTT_PASSWORD=pass
